<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center_horizontal"
    android:background="@color/pastelColor"
    tools:context=".Pantalla2VidaDeSanValentin">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical"
        android:orientation="vertical"
        android:padding="10sp">

        <LinearLayout
            android:id="@+id/idLayoutFotosCamino1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:padding="10sp">

            <ImageView
                android:id="@+id/idImagensiu"
                android:layout_width="50sp"
                android:layout_height="50sp"
                android:layout_marginRight="10sp"
                android:background="@drawable/sopa_de_letras_cuadrito"
                android:padding="3sp"
                android:src="@drawable/berriotxoa1" />

            <ImageView
                android:id="@+id/idImagensiu2"
                android:layout_width="50sp"
                android:layout_height="50sp"
                android:layout_marginRight="10sp"
                android:background="@drawable/sopa_de_letras_cuadrito"
                android:padding="3sp"
                android:src="@drawable/berriotxoa2" />

            <ImageView
                android:id="@+id/idImagensiu3"
                android:layout_width="50sp"
                android:layout_height="50sp"
                android:background="@drawable/sopa_de_letras_cuadrito"
                android:padding="3sp"
                android:src="@drawable/berriotxoa3" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/idLayoutFotosCamino2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:padding="10sp">

            <ImageView
                android:id="@+id/idImagensiu4"
                android:layout_width="50sp"
                android:layout_height="50sp"
                android:layout_marginRight="10sp"
                android:background="@drawable/sopa_de_letras_cuadrito"
                android:padding="3sp"
                android:src="@drawable/berriotxoa4" />

            <ImageView
                android:id="@+id/idImagensiu5"
                android:layout_width="50sp"
                android:layout_height="50sp"
                android:layout_marginRight="10sp"
                android:background="@drawable/sopa_de_letras_cuadrito"
                android:padding="3sp"
                android:src="@drawable/berriotxoa5" />

            <ImageView
                android:id="@+id/idImagensiu6"
                android:layout_width="50sp"
                android:layout_height="50sp"
                android:background="@drawable/sopa_de_letras_cuadrito"
                android:padding="3sp"
                android:src="@drawable/berriotxoa6" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/idLayoutFotosCamino3"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:padding="10sp">

            <ImageView
                android:id="@+id/idImagensiu7"
                android:layout_width="50sp"
                android:layout_height="50sp"
                android:layout_marginRight="10sp"
                android:background="@drawable/sopa_de_letras_cuadrito"
                android:padding="3sp"
                android:src="@drawable/berriotxoa7" />

            <ImageView
                android:id="@+id/idImagensiu8"
                android:layout_width="50sp"
                android:layout_height="50sp"
                android:layout_marginRight="10sp"
                android:background="@drawable/sopa_de_letras_cuadrito"
                android:padding="3sp"
                android:src="@drawable/berriotxoa8" />

            <ImageView
                android:id="@+id/idImagensiu9"
                android:layout_width="56dp"
                android:layout_height="48dp"
                android:background="@drawable/sopa_de_letras_cuadrito"
                android:padding="3sp"
                android:src="@drawable/berriotxoa9" />
        </LinearLayout>


    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical"
        android:layout_marginLeft="40sp"
        android:orientation="vertical"
        android:paddingLeft="20sp">

        <LinearLayout
            android:id="@+id/idLayoutCaminoFotos1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/idtv1"
                android:layout_width="50sp"
                android:layout_height="50sp"
                android:layout_marginRight="10sp"
                android:background="@drawable/sopa_de_letras_cuadrito"
                android:padding="3sp" />

            <ImageView

                android:layout_width="25sp"
                android:layout_height="25sp"
                android:layout_marginRight="10sp"
                android:src="@drawable/flechaderecha" />

            <TextView
                android:id="@+id/idtv2"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginRight="10sp"
                android:background="@drawable/sopa_de_letras_cuadrito"
                android:padding="3sp" />

            <ImageView

                android:layout_width="25sp"
                android:layout_height="25sp"
                android:layout_marginRight="10sp"
                android:src="@drawable/flechaderecha" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/idtv3"
                    android:layout_width="45dp"
                    android:layout_height="43dp"
                    android:layout_marginLeft="5sp"
                    android:layout_marginRight="10sp"
                    android:background="@drawable/sopa_de_letras_cuadrito"
                    android:padding="3sp" />

                <ImageView
                    android:layout_width="25sp"
                    android:layout_height="25sp"
                    android:layout_marginTop="8sp"
                    android:layout_marginRight="10sp"
                    android:rotation="90"
                    android:src="@drawable/flechaderecha" />

            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/idLayoutCaminoFotos2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="vertical"
                android:paddingLeft="5sp">

                <TextView
                    android:id="@+id/idtv4"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="3sp"
                    android:layout_marginRight="10sp"
                    android:background="@drawable/sopa_de_letras_cuadrito"
                    android:padding="3sp" />

                <ImageView
                    android:layout_width="25sp"
                    android:layout_height="25sp"
                    android:layout_marginTop="8sp"
                    android:layout_marginRight="10sp"
                    android:rotation="90"
                    android:src="@drawable/flechaderecha" />

            </LinearLayout>

            <ImageView
                android:layout_width="25sp"
                android:layout_height="25sp"
                android:layout_marginRight="10sp"
                android:rotation="180"
                android:src="@drawable/flechaderecha" />

            <TextView
                android:id="@+id/idtv5"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginRight="10sp"
                android:background="@drawable/sopa_de_letras_cuadrito"
                android:padding="3sp" />

            <ImageView
                android:layout_width="25sp"
                android:layout_height="25sp"
                android:layout_marginRight="10sp"
                android:rotation="180"
                android:src="@drawable/flechaderecha" />

            <TextView
                android:id="@+id/idtv6"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginRight="10sp"
                android:background="@drawable/sopa_de_letras_cuadrito"
                android:padding="3sp" />


        </LinearLayout>

        <LinearLayout
            android:id="@+id/idLayoutCaminoFotos3"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/idtv7"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginRight="10sp"
                android:background="@drawable/sopa_de_letras_cuadrito"
                android:padding="3sp" />

            <ImageView
                android:layout_width="25sp"
                android:layout_height="25sp"
                android:layout_marginRight="10sp"
                android:src="@drawable/flechaderecha" />

            <TextView
                android:id="@+id/idtv8"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginRight="10sp"
                android:background="@drawable/sopa_de_letras_cuadrito"
                android:padding="3sp" />

            <ImageView
                android:layout_width="25sp"
                android:layout_height="25sp"
                android:layout_marginRight="10sp"
                android:src="@drawable/flechaderecha" />

            <TextView
                android:id="@+id/idtv9"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginRight="10sp"
                android:background="@drawable/sopa_de_letras_cuadrito"
                android:padding="3sp" />

        </LinearLayout>


    </LinearLayout>

    <LinearLayout
        android:layout_width="271dp"
        android:layout_height="match_parent"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <Space
            android:layout_width="12sp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="horizontal" />

        <Button
            android:id="@+id/id_changeaudio1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="" />

        <SeekBar
            android:id="@+id/seekBar"
            android:layout_width="126dp"
            android:layout_height="46dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="23dp"
            android:layout_marginEnd="39dp"
            android:layout_marginBottom="83dp" />

        <Button
            android:id="@+id/id_changeaudio3"
            android:layout_width="209dp"
            android:layout_height="100dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="150dp"
            android:layout_marginTop="451dp"
            android:layout_marginEnd="159dp"
            android:layout_marginBottom="225dp"
            android:text="||" />

        <Button
            android:id="@+id/id_changeaudio2"
            android:layout_width="109sp"
            android:layout_height="100sp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="280dp"
            android:layout_marginTop="453dp"
            android:layout_marginEnd="29dp"
            android:layout_marginBottom="223dp"
            android:text="ez" />

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="50sp"
            android:layout_height="50sp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="75dp"
            android:layout_marginTop="74dp"
            android:layout_marginEnd="83dp"
            android:layout_marginBottom="401dp"
            android:src="@drawable/musique" />


    </LinearLayout>


</LinearLayout>