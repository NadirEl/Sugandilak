<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".juego2">
<LinearLayout
    android:id="@+id/lenao_img"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:gravity="center_horizontal"
    >
    <ImageView
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:src="@drawable/snv1"
        android:layout_marginRight="15dp"/>
    <ImageView
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:src="@drawable/snv2"
        android:layout_marginRight="15dp"/>
    <ImageView
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:src="@drawable/snv3"
        android:layout_marginRight="15dp"/>
    <ImageView
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:src="@drawable/snv4"
        android:layout_marginRight="15dp"/>
    <ImageView
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:src="@drawable/snv5"
        android:layout_marginRight="15dp"/>
    <ImageView
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:src="@drawable/snv6"
        android:layout_marginRight="15dp"/>
    <ImageView
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:src="@drawable/snv7"
        android:layout_marginRight="15dp"/>
    <ImageView
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:src="@drawable/snv8"
        android:layout_marginRight="15dp"/>
    <ImageView
        android:id="@+id/img_1"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:src="@drawable/snv9"
        android:layout_marginRight="15dp"/>


</LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/lenao_img"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/lenao_img"
            android:id="@+id/lenea1"
            android:orientation="horizontal">
            <ImageView
                android:layout_width="70dp"
                android:layout_height="70dp"
                app:srcCompat="@android:drawable/alert_light_frame"
                android:layout_marginRight="15dp"/>
            <ImageView
                android:layout_width="70dp"
                android:layout_height="70dp"
                app:srcCompat="@android:drawable/alert_light_frame"
                android:layout_marginRight="15dp"/>
            <ImageView
                android:layout_width="70dp"
                android:layout_height="70dp"
                app:srcCompat="@android:drawable/alert_light_frame"
                android:layout_marginRight="15dp"/>
        </LinearLayout>



        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/lenea1"
            android:id="@+id/lenea2"
            android:orientation="horizontal">
            <ImageView
                android:layout_width="70dp"
                android:layout_height="70dp"
                app:srcCompat="@android:drawable/alert_light_frame"
                android:layout_marginRight="15dp"/>
            <ImageView
                android:layout_width="70dp"
                android:layout_height="70dp"
                app:srcCompat="@android:drawable/alert_light_frame"
                android:layout_marginRight="15dp"/>
            <ImageView
                android:layout_width="70dp"
                android:layout_height="70dp"
                app:srcCompat="@android:drawable/alert_light_frame"
                android:layout_marginRight="15dp"/>
        </LinearLayout>




        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/lenea2"
            android:id="@+id/lenea3"
            android:orientation="horizontal">
            <ImageView
                android:layout_width="70dp"
                android:layout_height="70dp"
                app:srcCompat="@android:drawable/alert_light_frame"
                android:layout_marginRight="15dp"/>
            <ImageView
                android:layout_width="70dp"
                android:layout_height="70dp"
                app:srcCompat="@android:drawable/alert_light_frame"
                android:layout_marginRight="15dp"/>
            <ImageView
                android:layout_width="70dp"
                android:layout_height="70dp"
                app:srcCompat="@android:drawable/alert_light_frame"
                android:layout_marginRight="15dp"/>
        </LinearLayout>

    </LinearLayout>


</RelativeLayout>